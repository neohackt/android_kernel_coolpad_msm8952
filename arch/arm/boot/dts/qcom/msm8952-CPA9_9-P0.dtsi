#define BATTERY_LIMIT_VOLTAGE           (4400)
#define BATTERY_CV_THRESHOLD (BATTERY_LIMIT_VOLTAGE-10)
/ {
       aliases {
                i2c4 = &i2c_4;
		/delete-property/i2c4;
                /* Add I2C 7 for NXP NFC*/
                i2c7 = &i2c_7;
                /delete-property/ i2c5 ;
				spi6 = &spi_6;
        };
	memory {
    		pstore_reserve_mem: pstore_reserve_mem_region@0 {
            	linux,reserve-contiguous-region;
                linux,reserve-region;
                linux,remove-completely;
                reg = <0x0 0xdff00000 0x0 0x00100000>;//3GB DDR, will override at driver code for a right value, so dont take care this value. frankie. 2016-03-15.
                label = "pstore_reserve_mem";
        };
	};
};

/* Begin add bq25890 en configuration */
&i2c_2 { /* BLSP1 QUP2 */
             bq25890-parallel@6a {
                     compatible = "bq25890-parallel";
                     reg = <0x6a>;
                     bq,battery-voltage-mv = <BATTERY_LIMIT_VOLTAGE>;
                     bq,recharge-thresh-mv = <100>;
                     bq,bq25890-en = <&pmi8950_gpios 2 0>;
                  };

		/*KTD2026 Led*/
		ktd-leds@30 {
               	compatible = "ktd-leds";
                reg = <0x30>;
                ktd_leds,cfgs = <
                    0x07 /*ktd_red_out Register */
                    0x08 /*ktd_green_out Register*/
                    0x06 /*ktd_blue_out Register */

                    0x04 /*ktd_red_current,15mA */
                    0x04 /*ktd_green_current,15mA */
                    0x04 /*ktd_blue_current,15mA */
                    >;
                };
		/*fusb302 for type C*/
		fusb302@22 {
			compatible = "fusb302";
			reg = <0x22>;
			usb_det = <&msm_gpio 90 0x1>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <90 0>;
			interrupt-names = "usb_det_int";
			pinctrl-names = "default", "sleep";
			pinctrl-0 = <&fusb302_irq_active>;
			pinctrl-1 = <&fusb302_irq_sleep>;
		};
                /*add end*/
      };
&pmi8950_gpios {

         	gpio@c100 {
                     qcom,mode = <1>;
                     qcom,pull = <5>;
		     qcom,invert = <0>;
                     qcom,vin-sel = <0>;
		     qcom,src-sel = <2>;
                     qcom,master-en = <1>;
		     status = "okay";
               };
};
/* End add bq25890 en configuration */
