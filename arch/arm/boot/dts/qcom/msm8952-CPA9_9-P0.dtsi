/*huangqingjun add for xdja*/
&soc{
	spi_6: spi@0x7AF6000 {
			compatible = "qcom,spi-qup-v2";
			reg-names = "spi_physical", "spi_bam_physical";
			reg = <0x7AF6000 0x600>,
					<0x7AC4000 0x1f000>;
			interrupt-names = "spi_irq", "spi_bam_irq";
			interrupts = <0 300 0>, <0 239 0>;
			spi-max-frequency = <19200000>;
			pinctrl-names = "spi_default", "spi_sleep";
			pinctrl-0 = <&spi6_default &spi6_cs0_active &spi6_clk_default>;
			pinctrl-1 = <&spi6_sleep &spi6_cs0_sleep>;
			clocks = <&clock_gcc clk_gcc_blsp2_ahb_clk>,
				 <&clock_gcc clk_gcc_blsp2_qup2_spi_apps_clk>;
			clock-names = "iface_clk", "core_clk";
			qcom,infinite-mode = <0>;
			qcom,use-bam;
			qcom,use-pinctrl;
			qcom,ver-reg-exists;
			qcom,bam-consumer-pipe-index = <6>;
			qcom,bam-producer-pipe-index = <7>;
			qcom,master-id = <84>;
			#address-cells = <1>;
			#size-cells = <0>;

			ssx1207@0 {
				compatible = "xdja,ssx1207";
				reg = <0>;
				spi-max-frequency = <5000000>;
				spi-cpol = <0>;
				spi-cpha = <0>;
			};

		};


	tlmm_pinmux: pinctrl@1000000{
		spi6_active {
			/* MOSI, MISO */
			qcom,pins = <&gp 20>, <&gp 21>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <1>;
			label = "spi6-active";
			/* active state */
			spi6_default: spi6_default {
				drive-strength = <12>; /* 12 MA */
				bias-disable = <0>; /* No PULL */
			};
		};

		spi6_clk_active {
			/* CLK */
			qcom,pins = <&gp 23>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <1>;
			label = "spi6-clk-active";
			/* active state */
			spi6_clk_default: spi6_clk_default {
				drive-strength = <12>; /* 12 MA */
				bias-disable = <0>; /* No PULL */
			};
		};

		spi6_suspend {
			/* MOSI, MISO, CLK */
			qcom,pins = <&gp 20>, <&gp 21>, <&gp 23>;
			qcom,num-grp-pins = <3>;
			qcom,pin-func = <0>;
			label = "spi6-suspend";
			/* suspended state */
			spi6_sleep: spi6_sleep {
				drive-strength = <2>; /* 2 MA */
				bias-pull-down; /* pull down */
			};
		};
		spi6_cs0_active {
			/* CS */
			qcom,pins = <&gp 22>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <1>;
			label = "spi6-cs0-active";
			spi6_cs0_active: spi6_cs0_active {
				drive-strength = <2>;
				bias-disable = <0>;
			};
		};


		spi6_cs0_suspend {
			/* CS */
			qcom,pins = <&gp 22>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "spi6-cs0-suspend";
			spi6_cs0_sleep: spi6_cs0_sleep {
				drive-strength = <2>;
				bias-disable = <0>;
				};
			};
		};
	};


